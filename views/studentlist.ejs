<%- include('./base/header.ejs') %>
<div class="container">
    <div class="row">
        <table class="table text-center">
            <thead>
                <tr>
                    
                    <th>First Name </th>
                    <th>Last Name </th>
                    <th>Father Name</th>
                    <th>Roll No.</th>
                    <th>Aadhar card</th>
                    <th>Mobile No</th>
                    <th>Action</th>

                </tr>
            </thead>
            <% students.forEach((student)=>{%>
                <tr>
                    <td><%= student.firstName %></td>
                    <td><%= student.lastName %></td>
                    <td><%= student.fatherName %></td>
                    <td><%= student.rollNo %></td>
                    <td><%= student.aadharCardNo %></td>
                    <td><%= student.mobileNo %></td>
                    <td><a href="/edit/student/page/<%= student._id %>" class="btn btn-warning w-75">Edit</a></td>
                </tr>
                 <%})%>
        </table>

    </div>
</div>
<%- include('./base/footer.ejs') %>